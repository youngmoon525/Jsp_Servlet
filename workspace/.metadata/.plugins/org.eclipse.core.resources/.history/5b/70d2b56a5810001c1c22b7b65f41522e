<!-- jsp33.jsp?num=1	jsp33.jsp?num=2		jsp33.jsp?num=3 -->
<%
int num = Integer.parseInt(request.getParameter("num"));

if(num == 1){
	out.println("처음뵙겠습니다(java:if)");
}else if(num == 2){
	out.println("안녕하세요(java:if)");	
}else{
	out.println("반갑습니다(java:if)");	
}
%>
</body>
